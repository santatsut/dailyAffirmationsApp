{
  "expo": {
    "name": "DailyVerse",
    "slug": "Daily-Verse",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/inkFlow.png",
    "scheme": "dailyverse",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/inkFlow.png",
        "backgroundImage": "./assets/images/inkFlow.png",
        "monochromeImage": "./assets/images/inkFlow.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.DailyVerse"
    },
    "platforms": ["android", "web"],
    "web": {
      "output": "static",
      "favicon": "./assets/images/inkFlow.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/inkFlow.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0f118e2f-79ae-42ff-9648-f9de4a5f951b"
      }
    }
  }
}
